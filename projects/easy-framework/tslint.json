{
  "extends": "../../tslint.json",
  "rules": {
    "component-selector": [
      true,
      "element",
      "ez",
      "kebab-case"
    ],
    "directive-selector": [
      true,
      "attribute",
      "ez",
      "camelCase"
    ],
    "no-empty-interface": false,
    "no-magic-numbers": false,
    "no-non-null-assertion": false,
    "only-arrow-functions": false
  }
}
